{
  "compilerOptions": {
    "declaration": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./generated/tsc",
    "resolveJsonModule": true,
    "sourceMap": true,
    "target": "es6",
  },
  "compileOnSave": true,
  "exclude": [
    "**/node_modules",
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/dist"
  ],
  "include": ["./src/**/*"]
}